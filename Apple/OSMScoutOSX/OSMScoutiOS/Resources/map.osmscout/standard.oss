 // -------------------------------------------------------
 //
 // waterway_*
 //

 [MAG stateOver-] {
   [TYPE waterway_river] {
     [SIZE 10m 0.25mm:3px<] WAY {color: @waterColor; width: 10m;}
     [SIZE 10m <0.25mm:3px] WAY {color: @waterColor; displayWidth: 0.25mm; }
   }

  [TYPE waterway_riverbank] {
     AREA { color: @waterColor; }
   }
 }

 [MAG proximity-] {
   [TYPE waterway_intermittent_river] {
     [SIZE 10m 0.25mm:3px<] WAY {color: @waterColor; width: 10m;}
     [SIZE 10m <0.25mm:3px] WAY {color: @waterColor; displayWidth: 0.25mm; }
   }

   [TYPE waterway_canal, waterway_intermittent_canal] {
     [FEATURE Tunnel SIZE 5m 0.25mm:3px<] {
       WAY#outline { dash: 0.5,0.5; }
       WAY { color: lighten(@waterColor,0.3);}
     }
     [SIZE 5m 0.25mm:3px<] WAY {color: @waterColor; width: 5m;}
     [SIZE 5m <0.25mm:3px] WAY {color: @waterColor; displayWidth: 0.25mm; }
   }

   [TYPE waterway_intermittent_river, waterway_intermittent_canal] {
     WAY {dash: 2,1; joinCap: butt; }
   }

   [TYPE waterway_dock] {
     AREA { color: @waterColor; }
     AREA.TEXT { label: Name.name; }
   }

   [TYPE waterway_river, waterway_intermittent_river,
         waterway_canal, waterway_intermittent_canal] {
     WAY.TEXT { label: Name.name;}
   }

 }

 [MAG suburb-] {
   [TYPE waterway_stream, waterway_intermittent_stream] {
     [SIZE 2m 0.15mm:3px<] WAY {color: @waterColor; width: 2m;}
     [SIZE 2m <0.15mm:3px] WAY {color: @waterColor; displayWidth: 0.15mm;}
   }

   [TYPE waterway_drain, waterway_intermittent_drain] {
     [SIZE 3m 0.1mm:3px<] WAY {color: @waterColor; width: 3m;}
     [SIZE 3m <0.1mm:3px] WAY {color: @waterColor; displayWidth: 0.1mm; }
   }

   [TYPE waterway_intermittent_stream, waterway_intermittent_drain] {
     WAY {dash: 2,1; joinCap: butt; }
   }

  [TYPE waterway_waterfall] {
    NODE.ICON { name: waterfall; }
    NODE.TEXT { label: Name.name; size: 1.0; }
  }

 }

 [MAG block-] {
   [TYPE waterway_stream, waterway_intermittent_stream,
         waterway_drain, waterway_intermittent_drain]
     WAY.TEXT { label: Name.name;}
 }

 // -------------------------------------------------------
 //
 // highway_*, railway_*, route_ferry
 //

 [MAG stateOver-] {
   [TYPE highway_motorway] {
     [SIZE 10m 0.45mm:3px<] {
       WAY#outline { color: @motorwayBColor; width: 10m; displayWidth: 1.5mm; priority: -1; joinCap: butt; zIndex: -1; }
       WAY { color: @motorwayColor; width: 7m;}
     }
     [SIZE 10m <0.45mm:3px] WAY { color: @motorwayBColor; displayWidth: 0.45mm;}
   }

   [TYPE highway_trunk,highway_motorway_trunk, highway_motorway_primary] {
     [SIZE 10m 0.45mm:3px<] {
       WAY#outline { color: darken(@trunkColor,0.4); width: 12m; displayWidth: 0.5mm; priority: -1; joinCap: butt; zIndex: -1;}
       WAY { color: @trunkColor; width: 12m;}
     }
     [SIZE 10m <0.45mm:3px] WAY { color: lighten(@trunkColor,0.3); displayWidth: 0.4mm;}
   }

   [TYPE highway_primary] {
     [SIZE 10m 0.45mm:3px<] {
       WAY#outline { color: darken(@primaryColor,0.4); width: 10m; displayWidth: 0.5mm; priority: -1; joinCap: butt; zIndex: -1; }
       WAY { color: @primaryColor; width: 10m;}
     }
     [SIZE 10m <0.4mm:3px] WAY { color: lighten(@primaryColor,0.3); displayWidth: 0.4mm;}
   }

   [TYPE highway_secondary] {
     [SIZE 10m 0.45mm:3px<] {
       WAY#outline { color: darken(@secondaryColor,0.4); width: 8m; displayWidth: 0.5mm; priority: -1; joinCap: butt;  zIndex: -1;}
       WAY { color: @secondaryColor; width: 8m;}
     }
     [SIZE 8m <0.25mm:3px] WAY { color: #fdd654; displayWidth: 0.25mm; }
   }

 }


 [MAG proximity-] {
 }

