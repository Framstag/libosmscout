       WAY { color: @roadColor; width: 4m;}
     }
     [SIZE 4m <0.1mm:3px] WAY { color: #999999; displayWidth: 0.1mm;}
   }

   [TYPE highway_living_street] {
     [SIZE 4m 0.1mm:3px<] {
       WAY#outline { color: @roadBColor; width: 4m; displayWidth: 0.5mm; priority: -1; joinCap: butt; zIndex: -1; }
       WAY { color: @livingStreetColor; width: 4m;}
     }
     [SIZE 4m <0.1mm:3px] WAY { color: @livingStreetColor; displayWidth: 0.1mm;}
   }

   [TYPE highway_bus_guideway] WAY {color: #90570c; displayWidth: 0.1mm; }

   [TYPE railway_disused] WAY {color: #939393; dash: 1.5,1.5; joinCap: butt; displayWidth: 0.2mm; width: 4m;}

   [TYPE railway_tram] WAY {color: #444444; displayWidth: 0.25mm; width: 3m;}
   [TYPE railway_light_rail] WAY {color: #b3b3b3; displayWidth: 0.25mm; width: 3m;}

   [TYPE railway_subway] {
     WAY#outline { color: #ffffff50; width: 1.5m; displayWidth: 0.15mm; priority: -1; joinCap: butt; }
     WAY {color: #b3b3b3; width: 1.5m; dash: 20,10; joinCap: butt; }
   }

