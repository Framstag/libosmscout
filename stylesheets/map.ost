    = WAY ("railway"=="rail")
      {Bridge, Tunnel, Width}
      OPTIMIZE_LOW_ZOOM

  TYPE railway_tram
    = WAY ("railway"=="tram")
      {Bridge, Tunnel, Width}

  TYPE railway_light_rail
    = WAY ("railway"=="light_rail")
