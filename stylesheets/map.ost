        de: "Fußweg"

  TYPE highway_via_ferrata_easy
    = WAY (("highway"=="via_ferrata") AND (("via_ferrata_scale"=="0") OR ("via_ferrata_scale"=="0+") OR ("via_ferrata_scale"=="1-") OR ("via_ferrata_scale"=="1") OR ("via_ferrata_scale"=="1+")))
