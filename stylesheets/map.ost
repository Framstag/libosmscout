      PATH[FOOT BICYCLE CAR] LOCATION PIN_WAY
      DESC
        en: "living street"
        de: "Spielstraße"

  TYPE highway_service
    = WAY AREA ("highway"=="service")
      {Name, NameAlt, Sideway}
      PATH[FOOT BICYCLE CAR] LOCATION
      DESC
        en: "service"
        de: "Erschließungsweg"

  // Ways with main designation different to car

  TYPE highway_track
    = WAY AREA ("highway"=="track")
